    {
      "property": "Describe_input_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "geometry of ice sheets, ice shelves, land-ice,\nocean boundaries; material parameters;\nclimate forcings (i.e surface mass balance); basal friction at the ice/bed interface;\nflightlines; errors; boundaries; grids; preview\nimages"
        }
      ]
    },
    {
      "property": "Describe_key_physical_parameters",
      "dataitem": [
        {
          "type": 2,
          "item": "See https://issm.jpl.nasa.gov/"
        }
      ]
    },
    {
      "property": "Describe_length_scale_and_resolution",
      "dataitem": [
        {
          "type": 2,
          "item": "From hundreds of meters to thousands of kilometers (constraints are mainly HEC-related)."
        }
      ]
    },
    {
      "property": "Describe_numerical_limitations",
      "dataitem": [
        {
          "type": 2,
          "item": "Poor scaling for ice-flow models with direct solvers (improves upon use of iterative solvers, but convergence is not systematic)."
        }
      ]
    },
    {
      "property": "Describe_output_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "ice flow of an ice sheet, sea-level rise,\nvisco-elastic uplift"
        }
      ]
    },
    {
      "property": "Describe_processes",
      "dataitem": [
        {
          "type": 2,
          "item": "ice stress balance, ice mass transport / free surface, ice thermal (cold- and enthalpy-based), dual continuum hydrology, SHAKTI hydrology, GlaDS hydrology, ice damage mechanics, transient (time-dependent projection), grounding line dynamics, glacial isostatic adjustment (GIA), solid earth elastic response, sea-level fingerprints, positive degree day (PDD), surface energy balance (snow densification and surface mass balance calculation with the GEMB model), basal melt parameterizations (PICO/PICOP), empirical scalar tertiary anisotropy regime (ESTAR), uncertainty quantification capabilities (Dakota)"
        }
      ]
    },
    {
      "property": "Describe_time_scale_and_resolution",
      "dataitem": [
        {
          "type": 2,
          "item": "Typical time step of two weeks (constraints are mainly HEC-related)."
        }
      ]
    },
