      "property": "Describe_available_calibration_data",
      "dataitem": [
        {
          "type": 2,
          "item": "mosartwmpy was calibrated against historical MOSART-WM 30 year model runs."
        }
      ]
    },
    {
      "property": "Describe_available_test_data",
      "dataitem": [
        {
          "type": 2,
          "item": "Two years worth of input data is available to download with the model source code as a means to get familiar with the model. Other datasets are available upon request. Work is ongoing to provide generation utilities for input datasets from raw data sources."
        }
      ]
    },
    {
      "property": "Describe_ideal_data",
      "dataitem": [
        {
          "type": 2,
          "item": "A two year output dataset is available to download with the model source code as a means to validate and compare the effects of changes to the source code and input datasets."
        }
      ]
    },
    {
      "property": "Describe_input_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "river channel network, runoff, water demand, reservoir operations"
        }
      ]
    },
    {
      "property": "Describe_key_physical_parameters",
      "dataitem": [
        {
          "type": 2,
          "item": "river channel network geometry, flow direction, and flow coefficients -- Euler equations\nreservoir operations -- seasonal and data driven water release behavior"
        }
      ]
    },
    {
      "property": "Describe_length_scale_and_resolution",
      "dataitem": [
        {
          "type": 2,
          "item": "provided river channel parameters represent the CONUS at 1/8 degree resolution in WGS84 (epsg 4326); other resolutions possible"
        }
      ]
    },
    {
      "property": "Describe_output_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "water volume, water flux, reservoir storage, unmet water demand"
        }
      ]
    },
    {
      "property": "Describe_processes",
      "dataitem": [
        {
          "type": 2,
          "item": "water volume flux, water supply, reservoir operations"
        }
      ]
    },
    {
      "property": "Describe_time_scale_and_resolution",
      "dataitem": [
        {
          "type": 2,
          "item": "default settings represent a 3 hour time step for larger scale dynamics, and 1 hour time step for water volume flux subdivided into smaller iterations based on localized grid cell parameters (can get as small as ~12 minutes for some cells)"
        }
      ]
    },
