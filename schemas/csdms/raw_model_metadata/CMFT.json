    {
      "property": "Describe_input_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "Initial bottom configuration, wind and tide characteristics, sea level rise rate, water column sediment concentration at the boundary."
        }
      ]
    },
    {
      "property": "Describe_key_physical_parameters",
      "dataitem": [
        {
          "type": 2,
          "item": "In each cell and at each time step the following are computed: bottom elevation, above-ground vegetation, water level, wave height, tidal current velocity, bottom shear stress, and suspended sediment concentration."
        }
      ]
    },
    {
      "property": "Describe_length_scale_and_resolution",
      "dataitem": [
