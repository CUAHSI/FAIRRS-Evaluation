    {
      "property": "Describe_input_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "Initial elevation file.\n\nFile specifying boundary conditions, run time, process options, and parameter values."
        }
      ]
    },
    {
      "property": "Describe_key_physical_parameters",
      "dataitem": [
        {
          "type": 2,
          "item": "Bedrock erodibility, mass wasting diffusivity, bed material grain size, flow hydrologic parameters, relative evaporation rate, cratering size distribution and rate, eolian deposition parameters, etc."
        }
      ]
    },
    {
      "property": "Describe_length_scale_and_resolution",
      "dataitem": [
        {
          "type": 2,
          "item": "Variable - targeted towards drainage basin or larger scales. Spatial scale determined by input parameter and run-time specification of array dimentions. Uses rectangular grid cells."
        }
      ]
    },
    {
      "property": "Describe_numerical_limitations",
      "dataitem": [
        {
          "type": 2,
          "item": "Runs with grid sizes greater than about 600x600 may require many days on a PC.\nModel assumes fluvial streams have gradients determined by steady-state transport.\n\nDepositional stratigraphy not modeled."
        }
      ]
    },
    {
      "property": "Describe_output_parameters_model",
      "dataitem": [
        {
          "type": 2,
          "item": "RAW image files of elevation and shaded relief.\nASCII file of elevations at specified times.\nASCII files of other state variables as desired at specified times.\nIteration-by-iteration summary file"
        }
      ]
    },
    {
      "property": "Describe_post-processing_software",
      "dataitem": [
        {
          "type": 2,
          "item": "Photoshop to produce movies. Conversions to a variety of data formats."
        }
      ]
    },
    {
      "property": "Describe_processes",
      "dataitem": [
        {
          "type": 2,
          "item": "Bedrock fluvial incision (shear stress or sediment flux dependency).\nMass wasting (creep and threshold-limited).\nBedload sediment transport & deposition in streams, fans, deltas.\nImpact cratering, aeolian deposition, lava flows.\nFlow routing with evaporation from depressions."
        }
      ]
    },
    {
      "property": "Describe_time_scale_and_resolution",
